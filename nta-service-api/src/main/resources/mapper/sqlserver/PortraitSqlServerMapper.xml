<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hdu.mapper.sqlserver.PortraitSqlServerMapper">
    <select id="getInfoById" resultType="com.hdu.vo.UserPortraitsVO">
        select name,
               gender,
               mobile,
               RYLB,
               ZYMC,
               BJMC,
               YJSLB
        from easytong_plus.ZYYKT_USER
        where userId = #{adslAccount}
    </select>
</mapper>